{
    "name": "mobile-app-starter",
    "version": "1.0.0",
    "description": "Minimal (?) mobile app with JS/SCSS-linting using Webpack to build JS, SCSS and SVGs",
    "author": "NG",
    "license": "UNLICENCED",
    "main": "src/index.js",
    "scripts": {
        "build:travis": "cross-env NODE_ENV=production webpack --mode production",
        "build": "cross-env NODE_ENV=production webpack --mode production",
        "dev": "cross-env NODE_ENV=development webpack serve",
        "lint": "yarn lint:script && yarn lint:styles",
        "lint:script": "eslint \"src/**/*.{js,jsx}\" --fix --report-unused-disable-directives",
        "lint:styles": "stylelint 'src/**/*.scss' --fix",
        "start": "dotenv node server.js"
    },
    "dependencies": {
        "@ng-mw/reol": "^2.8.2",
        "@ng-mw/shared-app-components": "2.4.4",
        "@ng-mw/shared-react-components": "16.6.2",
        "@ng-mw/shared-webapp-server": "^10.11.3",
        "classnames": "^2.3.1",
        "core-js": "^3.22.6",
        "fontfaceobserver": "^2.3.0",
        "history": "^5.3.0",
        "prop-types": "^15.8.1",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-redux": "^8.0.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "redux": "^4.2.0",
        "redux-form": "^8.3.8",
        "regenerator-runtime": "^0.13.9"
    },
    "devDependencies": {
        "@babel/core": "^7.18.0",
        "@babel/plugin-proposal-class-properties": "^7.17.12",
        "@babel/plugin-proposal-decorators": "^7.17.12",
        "@babel/preset-env": "^7.18.0",
        "@babel/preset-react": "^7.17.12",
        "@ng-mw/eslint-config": "^2.0.12",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "cssnano": "^5.1.9",
        "dotenv": "^16.0.1",
        "file-loader": "^6.2.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.5.1",
        "mini-css-extract-plugin": "^2.6.0",
        "node-sass": "^7.0.1",
        "postcss-loader": "^7.0.0",
        "process": "^0.11.10",
        "pug-html-loader": "^1.1.5",
        "sass-loader": "^13.0.0",
        "sass-resources-loader": "^2.2.5",
        "stylelint": "^14.8.3",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "svg-sprite-loader": "^6.0.11",
        "svgo": "^2.8.0",
        "svgo-loader": "^3.0.0",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.0"
